  <body>

<!-- Update user settings -->
<!-- Modal -->
<div class="modal fade" id="Usersettings" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel"> <i class="fa fa-exclamation-triangle text-danger"></i> Settings <span id="delunamehd"></span></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="col-md-12">
            <form class="form-inline">
              <h4 style="text-align:center;margin:30px 0px;">Username Update</h4>
    <div class="form-group">

      <input type="text"  class="form-control mx-sm-3" id="oldusername" placeholder="username">

    </div><br>
  <div class="form-group"><br>

    <input type="text"  class="form-control mx-sm-3" id="newusername" placeholder="new username">

    <button type="button" name="button" class="btn btn-primary" onclick="UpdateUsername()">save</button>
    <span id="usernameErr" style="color:red"></span><span id="usernameSuccess" style="color:green"></span>
  </div>
</form>
          </div>
          <div class="col-md-12">
            <h4 style="text-align:center;margin:30px 0px;">Update password</h5>
            <form>
              <div class="form-row">
                <div class="form-group col-md-12">
                  <h5>Current password</h5>
                  <input type="password" class="form-control"  placeholder="current password" id="currentpwrd">
                </div>
                <div class="form-group col-md-12">
                  <h5>New password</h5>
                  <input type="password" class="form-control" placeholder="new password" id="newpwrd">
                </div>
                <div class="form-group col-md-12">
                  <h5 for="inputEmail4">Re enter new password</h5>
                  <input type="password" class="form-control"  placeholder="re enter new password" id="re_pwrd">
                  <span id="passwordErr" style="color:red"></span><span id="passwordSuccess" style="color:green"></span>
                </div>
                <div class="form-group col-md-6"><br>
                  <button type="button" name="button" class="btn btn-primary" onclick="UpdateUserPassword()">Update</button>

                </div>
              </div>

            </form>
          </div>
        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>

      </div>
    </div>
  </div>
</div>
<!-- /Update user settings -->
<nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#">CHAMBRE DE COMMERCE</a>
    </div>
    <div id="navbar" class="navbar-collapse collapse">

      <ul class="nav navbar-nav navbar-right">
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-user"></i>  
          <% if(user){ %><%= user.fname %><% } %> <span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li class="hidden" id="userid"><% if(user){ %><%= user.id %> <% } %></li>
            <li><a href="#"><% if(user){ %><%= user.fname %> <%= user.lname %><% } %></a></li>
            <li><a href="#" id="getemai"><% if(user){ %><%= user.email %><% } %></a></li>
            <li><a href="#" data-toggle="modal" data-target="#Usersettings">Settings</a></li>
            <li role="separator" class="divider"></li>
            <li><a href="/logout"><i class="glyphicon glyphicon-log-out"></i> Signout/
Déconnexion</a></li>

          </ul>
        </li>
      </ul>
    </div><!--/.nav-collapse -->
  </div><!--/.container-fluid -->
</nav>

    <div class="container">
      <div class="row row-offcanvas row-offcanvas-right">
        <div class="col-md-3  sidebar-offcanvas" id="sidebar">
          <div class="list-group">
            <a href="/ccima.cm/staffroom/home" class="list-group-item ">Companies</a>
            <a href="/ccima.cm/staffroom/home/registered_users" class="list-group-item">Users</a>
            <a href="/ccima.cm/staffroom/home/registerCompanies" class="list-group-item">Register Companies</a>
            <% if(user){ %>
              <% if(user.Admintype =='Super Admin'){ %>
                <a href="/ccima.cm/staffroom/home/users" class="list-group-item active" id="regUser" >Register Users</a>
              <% } %>
              <% } %>
          </div>
        </div><!--/.sidebar-offcanvas-->
        <div class="col-sm-9">
          <%- messages('message',locals) %>
          <h2 style="text-align:center">Register Users/
Enregistrer les utilisateurs  </h2>
          <div class="col-lg-8 col-lg-offset-2 well">
            <div class="row">
                <form action="/ccima.cm/staffroom/home/users" method="POST">
                  <div class="col-sm-12">
                    <div class="row">
                      <div class="col-sm-6 form-group">
                        <label>First Name/Prenom</label>
                        <input type="text" placeholder="First Name/Prenom.." class="form-control" name="fname" <% if( typeof fname != 'undefined'){ %> value= "<%= fname %>"<% } %> >
                        <% if( typeof fnameValidationErr != 'undefined'){ %><i class="err_msg"><%= fnameValidationErr %><% } %></i>
                      </div>
                      <div class="col-sm-6 form-group">
                        <label>Last Name/Nom</label><i>
                        <input type="text" placeholder="Last Name/Nom.." class="form-control" name="lname" <% if( typeof lname != 'undefined'){ %> value= "<%= lname %>"<% } %> >
                        <% if( typeof lnameValidationErr != 'undefined'){ %><i class="err_msg"><%= lnameValidationErr %><% } %></i>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-sm-6 form-group">
                        <label>Title/Titre</label><i>
                        <input type="text" placeholder="Enter title Here/
Entrez le titre ici.." class="form-control" name="title" <% if( typeof title != 'undefined'){ %> value= "<%= title %>"<% } %> >
                        <% if( typeof TitleValidationErr != 'undefined'){ %><i class="err_msg"><%= TitleValidationErr %><% } %></i>
                      </div>
                      <div class="col-sm-6 form-group">
                        <label>Company/Entreprise</label><i>
                        <input type="text" placeholder="Enter Company Name/
Entrez le nom de l'entreprise.." class="form-control" name="company" <% if( typeof company != 'undefined'){ %> value= "<%= company %>"<% } %> >
                        <% if( typeof CompanyValidationErr != 'undefined'){ %><i class="err_msg"><%= CompanyValidationErr %><% } %></i>
                      </div>
                    </div>
                    <div class="form-group">
                      <label>Administrator privileges/privilèges d'administrateur</label>
                      <select class="form-control" id="sell" name="adminType">
                        <option value="Administrator/Administrateur"> Administrator/Administrateur</option>
                        <option value="Super Admin">Super Administrator/Super Administrateur</option>
                      </select>
                    </div>
                  <div class="form-group">
                    <label>Phone Number/
Numéro de téléphone</label><i>
                    <input type="text" placeholder="enter phone number/
entrer le numéro de téléphone" class="form-control" name="pnum" <% if( typeof pnum != 'undefined'){ %> value= "<%= pnum %>"<% } %> >
                    <% if( typeof pnumValidationErr != 'undefined'){ %><i class="err_msg"><%= pnumValidationErr %><% } %></i>
                  </div>
                  <div class="form-group">
                    <label>Email Address</label><i>
                    <input type="text" placeholder="Email Address.." class="form-control" name="email" <% if( typeof email != 'undefined'){ %> value= "<%= email %>"<% } %> >
                    <% if( typeof EmailValidationErr != 'undefined'){ %><i class="err_msg"><%= EmailValidationErr %><% } %> &nbsp;<% if( typeof nEmailValidationErr != 'undefined'){ %><%= nEmailValidationErr %><% } %></i>
                  </div>
                  <input type="submit"  class="btn btn-lg btn-info" name="reguser">

                  </div>
                </form>
              </div>
            </div>
        </div>


      </div><!--/row-->


      </div><!--/row-->
    <!-- Footer -->
    <footer class="py-5 bg-dark">
      <div class="container">
        <p class="m-0 text-center text-white">Copyright &copy; 2018</p>
      </div>
      <!-- /.container -->
    </footer>
